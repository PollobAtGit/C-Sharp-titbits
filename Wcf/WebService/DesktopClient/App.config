<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="WebServiceOneSoap" />
                <binding name="BasicHttpBinding_IHelloService" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_IHelloService" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:25645/WebServiceOne.asmx"
                binding="basicHttpBinding" bindingConfiguration="WebServiceOneSoap"
                contract="HelloWorldServiceClient.WebServiceOneSoap" name="WebServiceOneSoap" />
            <endpoint address="http://localhost:8080/helloservice" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IHelloService" contract="HelloServiceWCF.IHelloService"
                name="BasicHttpBinding_IHelloService" />
            <endpoint address="net.tcp://localhost:8090/helloservice" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IHelloService" contract="HelloServiceWCF.IHelloService"
                name="NetTcpBinding_IHelloService">
                <identity>
                    <userPrincipalName value="POLLOB\pollo_000" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>